{
  "name": "@repo/astro-theroks-blog",
  "description": "TheRoks Blog - A modern Astro blog with PNPM workspace",
  "version": "0.9.8",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter @repo/blog dev",
    "start": "pnpm --filter @repo/blog start",
    "build": "pnpm --filter @repo/blog build",
    "preview": "pnpm --filter @repo/blog preview",
    "astro": "pnpm --filter @repo/blog astro",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "lint": "eslint .",
    "lint:eslint": "eslint .",
    "typecheck": "pnpm -r run typecheck"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "globals": "16.4.0",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0"
  },
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "pnpm": {
    "overrides": {
      "form-data@2.x.x": "^4.0.0",
      "ws@8.x.x": "^8.17.1",
      "tar-fs@2.x.x": "^3.0.0"
    }
  }
}
